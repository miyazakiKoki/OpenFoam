FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setParticlePositionsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

positioningType        cubic;


// ****** subDict ****** //
cubicPositionSubDict
{
    particleDiameter	0.00156;
    particleNumber        100;
    boundaryLengthX       1;
    boundaryLengthY       1;
    boundaryLengthZ       1;
    firstPositionX        0.5;
    firstPositionY        0.5;
    firstPositionZ        0.5;
}

